<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.koreanapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/img_koreann"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WonderVN.PlaceInformationActivity" android:parentActivityName=".WonderVN.MainActivity"></activity>
        <activity android:name=".WonderVN.PromotionActivity" />
        <activity android:name=".WonderVN.ContactActivity" />
        <activity android:name=".WonderVN.PlaceActivity" />
        <activity android:name=".WonderVN.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>